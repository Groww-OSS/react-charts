{
  "name": "@groww-tech/react-charts",
  "version": "0.0.3",
  "description": "React charts",
  "main": "lib/react-charts.min.js",
  "types": "types/index.d.ts",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "types"
  ],

  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Groww/react-charts.git"
  },
  "author": "Vikas Singh <vikas.singh707@gmail.com> (https://twitter.com/VikasSi25579022)",
  "contributors": [
    "Samyak Jain (https://twitter.com/__samyak)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Groww/react-charts/issues"
  },
  "homepage": "https://github.com/Groww/react-charts/blob/main/README.md",
  "scripts": {
      "build": "yarn build:commonjs && yarn build:es && yarn build:types",
      "build:commonjs": "rimraf ./lib && rollup -c && rollup-plugin-visualizer stats-react.json",
      "build:es": "rimraf ./es && babel --extensions .ts,.tsx  ./src --out-dir es",
      "build:types": "rimraf ./types && tsc --project ./tsconfig.types.json",
      "watch": "yarn watch:commonjs & yarn watch:es & yarn watch:types",
      "watch:commonjs": "rimraf ./lib && cross-env BABEL_ENV=commonjs babel --watch --extensions .ts,.tsx ./src --out-dir lib",
      "watch:es": "rimraf ./es && babel --watch --extensions .ts,.tsx ./src --out-dir es",
      "watch:types": "rimraf ./types && tsc --watch --project ./tsconfig.types.json",
      "now-build": "yarn && yarn build",
      "start": "yarn watch",
      "stats": "open ./stats.html"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "@groww-tech/base-css": "0.0.3",
    "prop-types": "15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@groww-tech/eslint-config": "1.0.4",
    "@groww-tech/eslint-plugin-internal": "1.0.2",
    "@groww-tech/tsconfig": "^1.0.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/d3-array": "^3.0.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "postcss": "^8.2.13",
    "postcss-import": "^14.0.2",
    "postcss-url": "^10.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^4.2.2",
    "@babel/cli": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-prettier": "^2.1.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-visualizer": "^5.5.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@visx/axis": "^2.2.2",
    "@visx/event": "^2.1.2",
    "@visx/group": "^2.1.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.2.2"
  },
  "keywords": [
    "charts",
    "react-charts",
    "groww",
    "charts",
    "candle-chart",
    "line-chart",
    "bar-graph",
    "pie-cchart"
  ]
}
