  "scripts": {
    "build": "yarn build:commonjs && yarn build:es && yarn build:types",
    "build:commonjs": "rimraf ./lib &&  cross-env NODE_ENV=production rollup -c && rollup-plugin-visualizer stats-react.json",
    "build:types": "rimraf ./types && tsc --project ./tsconfig.types.json",
    "watch": "yarn watch:commonjs & yarn watch:es & yarn watch:umd & yarn watch:types",
    "watch:commonjs": "rimraf ./lib && cross-env BABEL_ENV=commonjs babel --watch --extensions .ts,.tsx ./src --out-dir lib",
    "watch:es": "rimraf ./es && babel --watch --extensions .ts,.tsx ./src --out-dir es",
    "watch:umd": "rimraf ./dist && cross-env NODE_ENV=production rollup -w -c && rollup-plugin-visualizer stats-react.json",
    "watch:types": "rimraf ./types && tsc --watch --project ./tsconfig.types.json",
    "now-build": "yarn && cd www && yarn && yarn build",
    "start": "yarn watch",
    "format": "prettier {.,src,src/**,example/src,example/src/**,types}/*.{md,js,jsx,tsx,json} --write",
    "stats": "open ./stats.html",
    "typedoc": "typedoc"
  },
